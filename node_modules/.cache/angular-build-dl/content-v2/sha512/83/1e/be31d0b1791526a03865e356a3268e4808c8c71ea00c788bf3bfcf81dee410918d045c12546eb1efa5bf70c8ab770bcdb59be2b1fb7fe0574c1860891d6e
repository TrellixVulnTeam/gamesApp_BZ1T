s = this.db.fetchShipsOwner();    \n      }\n    });\n\n  }\n\n  destroyShip(ship:Ship){\n    this.db.destroyShips(ship.id).then((res) => {\n      console.log(res);\n    })\n  }\n\n  async presentModal(e) {\n    const modal = await this.modalController.create({\n      component: ModalShipPage,\n      cssClass: 'modal',\n      componentProps: {\n        Attack_Strength: e.Attack_Strength,\n        Buy: e.Buy,\n        CP: e.CP,\n        Class: e.Class,\n        Defense_Strength: e.Defense_Strength,\n        Description: e.Description,\n        Hull_Size: e.Hull_Size,\n        TAttack: e.TAttack,\n        TDefense: e.TDefense,\n        TMove: e.TMove,\n        TOther: e.TOther,\n        TTactics: e.TTactics,\n        Type: e.Type,\n        id: e.id\n      }\n    });\n    return await modal.present();\n\n  }\n\n  \n  navigateToSapce(){\n    this.router.navigateByUrl('/games/space');\n  }\n\n  \n  shipFake:any[] =[\n  {\n    Attack_Strength: \"E3 (A6-8)\",\n    Buy: 1,\n    CP: 6,\n    Class: \"Scout\",\n    Defense_Strength: \"0\",\n    Description: \"May have increased firepower versus fighters depending on level of point defense technology - A6 at PD1, A7 at PD2, A8 at PD3.\",\n    Hull_Size: 1,\n    TAttack: 0,\n    TDefense: 0,\n    TMove: 1,\n    TOther: null,\n    TTactics: 0,\n    Type: \"SC #1\",\n    id: 1\n  },\n  {\n    Attack_Strength: \"E3 (A6-8)\",\n    Buy: 1,\n    CP: 6,\n    Class: \"Scout\",\n    Defense_Strength: \"0\",\n    Description: \"May have increased firepower versus fighters depending on level of point defense technology - A6 at PD1, A7 at PD2, A8 at PD3.\",\n    Hull_Size: 1,\n    TAttack: 0,\n    TDefense: 0,\n    TMove: 1,\n    TOther: null,\n    TTactics: 0,\n    Type: \"SC #2\",\n    id: 2\n  }\n]\n\n}\n","export default \".headerSpace {\\n  display: contents;\\n}\\n\\n.headerTitle {\\n  text-align: center;\\n  margin: 2rem;\\n}\\n\\nion-menu-button {\\n  color: var(--ion-color-primary);\\n}\\n\\n.caja {\\n  text-align: center;\\n}\\n/*